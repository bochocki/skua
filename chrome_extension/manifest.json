{
  "name": "Skua",
  "version": "0",
  "description": "A smart filter for abusive tweets.",
  "background": {"page": "background.html"},
  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["birdhead.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": [ "jquery-2.0.2.min.js", "background.js" ],
    "css": ["customStyles.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
